      "source": [
        "# código para gerar um tabuada conforme entrada do usuário\n",
        "x = input('Digite a tabuada que deseja: ')\n",
        "x = int(x)\n",
        "print(f'\\n\\tTABUADA DO {x}\\n')\n",
        "\n",
        "for i in range(11):\n",
        "  resultado = i * x\n",
        "  print(f'\\t{x} X', i, '=', resultado)"
      ],
